#include <stdio.h>
int main(){
    int n,found=0;
    printf("enter the size of the array:");
    scanf("%d",&n);
    int a[n];
    printf("enter the elements in the array:");
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("enter the key u need check:");
    int key;
    scanf("%d",&key);
for(int i=0;i<n;i++){
    if(a[i]==key){
        printf("element is found at index is: %d",i);
        found=1;
        break;
    }
    }
if(!found){
        printf("element is  not found:");
}
       return 0;
}
